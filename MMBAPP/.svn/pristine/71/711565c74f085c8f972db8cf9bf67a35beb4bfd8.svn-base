/*product-list E*/
.layout .alert{
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(1,1,1,0.5);
    z-index:1000;
    display: none;
}
.alert>.tip{
    width: 300px;
    height: 150px;
    margin: 200px auto;
    color: #FFFFFF;
    border:2px solid #CF6B10;
}
.alert>.tip>.title{
    background-color: #ff841d;
    height: 30px;
    line-height: 30px;
    padding-left: 10px;
    position: relative;
}
.alert>.tip>.con{
    background-color: #fff;
    color: #ff841d;
    height: 116px;
    line-height: 116px;
    font-size: 20px;
    font-weight: 900;
    text-align: center;
}
.alert>.tip>.title>span{
    font-size: 20px;
    width: 20px;
    height: 20px;
    background-color: #FFFFFF;
    color: #ff841d;
    position: absolute;
    border-radius: 50%;
    text-align: center;
    line-height: 18px;
    right: 5px;
    top: 5px;

}

.fixedNav {
    z-index: 999;
    bottom: 150px;
    right: 0;
    position: fixed;
    width: 40px;
    height: 40px;
    background: url("../images/coudanhome.svg");
}
.fixedNav .menu {
    display: none;
    border: 4px solid #d3d3d3;
    border-radius: 14px;
    background: #f2f2f2;
    position: fixed;
    min-width: 60%;
    width: 60%;
    top: 0;
    left: 50px;
}
.fixedNav .menu ul {
    padding-bottom: 15px;
}
.fixedNav .menu ul li {
    display: block;
    text-align: center;
    float: left;
    width: 25%;
}
.fixedNav .menu ul li a {
    font-size: 14px;
    display: block;
    color: #000;
}
.fixedNav .menu ul li a img {
    display: block;
    width: 100%;
    padding: 15px 15px 8px 15px;
}
.fixedNav .menu ul li a p {
    height: 20px;
    line-height: 20px;
    overflow: hidden;
}