html,
body {
  min-width: 320px;
}
.header-bar {
  height: 44px;
  background-color: #ff8b00;
  overflow: hidden;
  position: relative;
  box-shadow: 0px 2px 3px #dddddd;
}
.header-bar.border-bottom-img {
  border-bottom: 1px solid #dadada;
  border-width: 0 0 1px 0;
}
.header-bar .inner {
  padding: 0 8px;
}
.header-bar .inner .left {
  position: absolute;
  left: 8px;
  bottom: 0px;
  height: 44px;
  width: 44px;
  line-height: 44px;
  color: #FFFFFF;
}
.header-bar .inner .left .icon-back {
  box-sizing: border-box;
  background: url("../images/leftArrow.svg") no-repeat;
  width: 44px;
  height: 44px;
  background-position: center center;
  display: block;
  padding: 10px;
  background-origin: content-box;
  background-clip: content-box;
}
.header-bar .center {
  text-align: center;
}
.header-bar .center h1 {
  font-size: 19px;
  line-height: 44px;
  color: #ffffff;
  margin: 0px;
  padding: 0px;
  font-weight: normal;
  padding: 0px 62px;
}
.header-bar .right {
  position: absolute;
  right: 8px;
  bottom: 0px;
  height: 44px;
  width: 44px;
  line-height: 44px;
  color: #FFFFFF;
  text-align: right;
}
.Page_loading {
  margin-top: 15px;
  text-align: center;
  font-family: "微软雅黑";
  font-size: 16px;
  background-color: #f8f8f8;
  display: none;
}
.Page_loading em {
  margin-top: 15px;
  vertical-align: middle;
  margin-right: 4px;
}
.coudan-box .list .hd {
  position: relative;
  height: 40px;
  display: -webkit-box;
}
.coudan-box .list .hd .filter {
  width: 100%;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-flex: 1;
  border-bottom: 1px solid #dadada;
}
.coudan-box .list .hd .filter ul {
  width: 100%;
}
.coudan-box .list .hd .filter ul li {
  border-right: 2px solid #ff8b00;
  float: left;
  line-height: 40px;
  width: 33.33333%;
  display: -webkit-box !important;
  position: relative;
  -webkit-box-flex: 1;
  box-flex: 1;
  box-sizing: border-box;
}
.coudan-box .list .hd .filter ul li a {
  display: -webkit-box !important;
  position: relative;
  width: 100%;
  font-size: 17px;
  text-align: center;
  -webkit-box-align: center;
  box-align: center;
  -webkit-box-pack: center;
  box-pack: center;
}
.coudan-box .list .hd .filter ul li i {
  display: inline-block;
  width: 12px;
  height: 12px;
  margin: 6px 0 0 10px;
  vertical-align: middle;
  border-color: #666 transparent transparent transparent;
  border-width: 6px;
  border-style: solid;
  transition: all .3s;
  transform-origin: 6px 3px;
  -webkit-transform-origin: 6px 3px;
}
.coudan-box .list .hd .filter ul li i.on {
  transform: rotate(180deg);
  -webkit-transform: rotate(180deg);
}
.coudan-box .list .hd .search {
  width: 46px;
  position: relative;
  border-bottom: 1px solid #dadada;
  text-align: center;
}
.coudan-box .list .hd .search i.icon-search {
  display: inline-block;
  width: 40px;
  height: 40px;
  background-image: url("../images/search.svg");
  background-repeat: no-repeat;
  color: #ffffff;
  background-size: 22px 22px;
  background-position: center center;
}
.coudan-box .list .hd .popbox {
  width: 100%;
  background-color: #ffffff;
  position: absolute;
  left: 0px;
  top: 40px;
  box-shadow: 0px 3px 6px #dddddd;
  display: none;
  z-index: 10;
}
.coudan-box .list .hd .popbox ul li {
  border-bottom: 1px solid #dadada;
  border-width: 0 0 1px 0;
  position: relative;
}
.coudan-box .list .hd .popbox ul li a {
  position: relative;
  z-index: 2;
  display: block;
  line-height: 50px;
  padding: 0px 10px;
  font-size: 15px;
}
.coudan-box .list .hd .popbox li.on:after {
  content: "";
  display: block;
  color: #ff8b00;
  width: 20px;
  height: 20px;
  background-image: url("../images/tick.svg");
  background-repeat: no-repeat;
  -webkit-background-size: 20px auto;
  position: absolute;
  right: 15px;
  top: 50%;
  margin-top: -10px;
}
.coudan-box .list .bd {
  background-color: #f1f1f1;
  padding: 6px 0px;
}
.coudan-box .list .bd ul {
  position: relative;
  width: 98%;
  margin: 0px auto;
}
.coudan-box .list .bd ul li {
  float: left;
  width: 50%;
}
.coudan-box .list .bd a {
  display: block;
  margin: 4px;
  background-color: #ffffff;
}
.coudan-box .list .bd .pic {
  width: 100%;
  height: auto;
  text-align: center;
  overflow: hidden;
}
.coudan-box .list .bd .title {
  color: #333333;
  word-break: break-all;
  padding: 0 6px;
  height: 14px;
  overflow: hidden;
  font-size: 12px;
  margin-top: 10px;
  line-height: 14px;
  font-weight: normal;
  text-align: left;
  margin-top: 6px;
}
.coudan-box .list .bd .other {
  padding: 6px;
}
.coudan-box .list .bd .other .price {
  float: left;
  color: #f8601d;
  font-size: 20px;
}
.coudan-box .list .bd .other .price::first-letter {
  font-style: normal;
  font-size: 10px;
  color: #f8601d;
}
.coudan-box .list .bd .other .btn {
  float: right;
  font-size: 15px;
  color: #FFFFFF;
  border-radius: 3px;
  padding: 4px 10px 4px 28px;
  text-decoration: none;
  display: inline-block;
  position: relative;
}
.coudan-box .list .bd .other .btn span {
  position: absolute;
  left: 10px;
  top: 6px;
  height: 16px;
  width: 16px;
}
.coudan-box .list .bd .other .btn.jd {
  background-color: #C81522;
}
.coudan-box .list .bd .other .btn.jd span {
  background: url("../images/JD_logo.svg");
}
.coudan-box .list .bd .other .btn.yhd {
  background-color: #D01117;
}
.coudan-box .list .bd .other .btn.yhd span {
  background: url("../images/yihaodian.svg");
}
.coudan-box .list .bd .other .btn.tmall {
  background-color: #BF0000;
}
.coudan-box .list .bd .other .btn.tmall span {
  background: url("../images/tmall_logo.svg");
}
.coudan-box .list .bd img {
  max-width: 100%;
  width: 100%;
  height: auto;
}
.coudan-box .form {
  font-size: 15px;
  color: #5a5a5a;
}
.coudan-box .form keygen,
.coudan-box .form select,
.coudan-box .form select[size="0"],
.coudan-box .form select[size="1"] {
  border-radius: 0px;
  border-color: #a9a9a9;
}
.coudan-box .form select {
  -webkit-appearance: none;
  font-size: 15px;
  margin-left: 4px;
  height: 44px;
  line-height: normal;
  width: 102px;
  border: none;
  text-align: right;
  vertical-align: top;
}
.coudan-box .form option {
  font-weight: normal;
  display: block;
  white-space: pre;
  min-height: 1.2em;
  padding: 0px 2px 1px;
}
.coudan-box .form .list-title {
  position: relative;
  overflow: hidden;
  margin: 0;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 15px;
  text-transform: uppercase;
  line-height: 1;
  padding: 20px 15px 10px 15px;
}
.coudan-box .form .list-block {
  margin: 35px 0;
  font-size: 15px;
  margin-top: 10px;
}
.coudan-box .form .list-block ul {
  background: #fff;
  list-style: none;
  padding: 0;
  margin: 0;
  position: relative;
}
.coudan-box .form .list-block ul li {
  padding-left: 15px;
  min-height: 44px;
  line-height: 44px;
  position: relative;
}
.coudan-box .form .list-block ul li:after {
  background: url("../images/couyixia_rightArrow.svg") right center no-repeat;
  background-size: 10px 20px;
  content: "";
  display: block;
  width: 9px;
  height: 15px;
  position: absolute;
  right: 15px;
  top: 50%;
  margin-top: -7px;
}
.coudan-box .form .list-block ul li.p:after {
  background: none;
}
.coudan-box .form .list-block ul li.p a.a-delput {
  width: 20px;
  height: 20px;
  border-radius: 10px;
  background-color: #d1d1d1;
  opacity: 0;
  position: absolute;
  right: 10px;
  top: 12px;
  -webkit-background-size: contain;
  background-size: contain;
  -webkit-transition-duration: 300ms;
  transition-duration: 300ms;
  cursor: pointer;
  z-index: 3;
}
.coudan-box .form .list-block .media {
  float: left;
}
.coudan-box .form .list-block .media i.icon-money {
  background-color: #ff3b13;
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' v…12H752.3387602097464z' transform='translate(0, 812) scale(1, -1)'/></svg>");
  background-repeat: no-repeat;
  color: #ffffff;
  background-size: 17px 17px;
  background-position: center center;
}
.coudan-box .form .list-block .title {
  float: right;
  margin-right: 15px;
}
.coudan-box .form .list-block .title span {
  display: inline-block;
}
.coudan-box .form .list-block .title span select {
  -webkit-appearance: none;
  font-size: 15px;
  margin-left: 4px;
  height: 44px;
  line-height: normal;
  width: 102px;
  border: none;
  text-align: right;
}
.coudan-box .form .p input {
  -webkit-appearance: none;
  width: 76px;
  height: 44px;
  line-height: normal;
  font-size: 15px;
  border: none;
  margin-right: 20px;
  vertical-align: top;
}
.coudan-box .form .b {
  padding: 0px 15px;
}
.coudan-box .form .b a.a-coubtn {
  display: block;
  background-color: #fe7b07;
  text-align: center;
  color: #ffffff;
  font-size: 18px;
  height: 40px;
  line-height: 40px;
  border-radius: 4px;
  font-weight: normal;
}
.border-top-img {
  border-top: 1px solid #dadada;
  border-width: 1px 0 0 0;
}
.border-bottom-img {
  border-bottom: 1px solid #dadada;
  border-width: 0 0 1px 0;
}
.border-img {
  border-width: 0;
  border: 1px solid #eeeeee;
}
.backToTop {
  display: none;
  right: 10px;
  bottom: 14px;
  position: fixed;
  padding: 8px;
  box-sizing: content-box;
  height: 32px;
  width: 32px;
  background-image: url("../images/backToTop.svg");
  background-repeat: no-repeat;
  background-origin: content-box;
  border: 2px solid #bbb;
  border-radius: 50%;
  opacity: .85;
  background-color: #fff;
}
.update {
  display: none;
  position: fixed;
  z-index: 4;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.7);
}
.update ul {
  position: relative;
  width: 300px;
  height: 400px;
  background: #ff8b00;
  color: #fff;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  -webkit-transform: translateX(-50%) translateY(-50%);
  border-radius: 14px;
  padding: 20px;
  font-size: 18px;
  animation: bling .7s infinite alternate;
}
.update ul li {
  line-height: 30px;
  margin-bottom: 10px;
  padding-left: 20px;
}
.update ul li:first-of-type,
.update ul li:last-of-type {
  padding-left: 0px;
}
.update ul li:first-of-type {
  font-weight: bold;
}
.update ul button {
  height: 40px;
  background: #fff;
  border: 0;
  color: #dc7400;
  font-size: 16px;
  font-weight: bold;
  display: block;
  width: 100%;
  border-radius: 5px;
}
.fixedNav {
  bottom: 150px;
  right: 0;
  position: fixed;
  width: 40px;
  height: 40px;
  background: url("../images/coudanhome.svg");
}
.fixedNav .menu {
  display: none;
  border: 4px solid #d3d3d3;
  border-radius: 14px;
  background: #f2f2f2;
  position: fixed;
  min-width: 60%;
  width: 60%;
  top: 0;
  left: 50px;
}
.fixedNav .menu ul {
  padding-bottom: 15px;
}
.fixedNav .menu ul li {
  display: block;
  text-align: center;
  float: left;
  width: 25%;
}
.fixedNav .menu ul li a {
  font-size: 14px;
  display: block;
  color: #000;
}
.fixedNav .menu ul li a img {
  display: block;
  width: 100%;
  padding: 15px 15px 8px 15px;
}
.fixedNav .menu ul li a p {
  height: 20px;
  line-height: 20px;
  overflow: hidden;
}
@keyframes bling {
  to {
    box-shadow: 0 0 20px #ffad00;
  }
}
