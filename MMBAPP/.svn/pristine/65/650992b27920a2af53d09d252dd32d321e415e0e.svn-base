.mySearch {
    width: 100%;
    height: 56px;
    background-color: #fff;
    margin: 0 auto;
    position: relative;
    padding: 10px;
}
.serach_t {
    width: 100%;
    position: relative;
    line-height: 36px;
}
.serach_t input {
    border: 0 none;
    width: 100%;
    height: 36px;
    padding-right: 60px;
    background-color: #F1F1F1;
    font-size: 14px;
    overflow: hidden;
    border-radius: 18px;
    padding-left: 10px;
    font-weight: 500;
}
.serach_t a {
    border-radius: 0 18px 18px 0;
    text-align: center;
    right: 0;
    top: 0;
    background-color: #FF841D;
    width: 60px;
    height: 36px;
    position: absolute;
    color: #fff;
    font-size: 20px;
}
.mySearch .box {
    top: 56px;
    left: 0;
    position: absolute;
    width: 100%;
    max-height: 300px;
    overflow: scroll;
    z-index: 999;
    display: none;
}
.mySearch .box ul {
    width: 100%;
}
.mySearch .box ul li {
    overflow: hidden;
    width: 100%;
    height: 30px;
    font-size: 13px;
    padding-left: 10px;
    line-height: 30px;
}
.mySearch .box ul li:nth-of-type(odd) {
    background-color: #fff;
}
.mySearch .box ul li:nth-of-type(even) {
    background-color: #f8f8f8;
}